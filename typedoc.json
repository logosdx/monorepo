{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["packages/*"],
    "out": "./typedoc",
    "name": "Logos DX",
    "entryPointStrategy": "packages",
    "includeVersion": true,
    "plugin": [
        "typedoc-plugin-missing-exports",
        "typedoc-plugin-mdn-links"
    ],
}